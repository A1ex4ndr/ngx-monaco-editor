{
  "name": "ngx-monaco-editor",
  "version": "10.0.0-beta.1",
  "description": "Monaco Code Editor For Angular",
  "keywords": [
    "angular",
    "monaco",
    "monaco-editor",
    "code",
    "editor",
    "reusable"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --project=editor-tester --base-href=editor-tester",
    "build:prod": "ng build --prod --project=editor-tester --base-href=editor-tester",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-editor": "npm run copy:readme && ng build editor --prod",
    "npm-publish": "npm run build-editor && bash npm-publish",
    "copy:readme": "cp -rf README.md projects/editor/"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/atularen/ngx-monaco-editor.git"
  },
  "bugs": {
    "url": "https://github.com/atularen/ngx-monaco-editor/issues"
  },
  "author": "Atul Kumar",
  "contributors": [
    "Atul Kumar <atulaggarwal4288@gmail.com>"
  ],
  "dependencies": {
    "@angular/animations": "~10.1.0",
    "@angular/common": "~10.1.0",
    "@angular/compiler": "~10.1.0",
    "@angular/core": "~10.1.0",
    "@angular/forms": "~10.1.0",
    "@angular/platform-browser": "~10.1.0",
    "@angular/platform-browser-dynamic": "~10.1.0",
    "@angular/router": "~10.1.0",
    "monaco-editor": "^0.20.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.0",
    "@angular/cli": "~10.1.0",
    "@angular/compiler-cli": "~10.1.0",
    "@angular/language-service": "~10.1.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "ng-packagr": "^10.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
